<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
	http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">

	<bean id="customerDAO" class="main.java.com.getsetgo.dao.impl.JdbcCustomerDAO">
		<property name="dataSource" ref="dataSource" />
	</bean>
	
	<bean id="userDAO" class="main.java.com.getsetgo.dao.impl.JdbcUserDAO">
		<property name="dataSource" ref="dataSource" />
	</bean>
	
	<bean id="tournamentDAO" class="main.java.com.getsetgo.dao.impl.JdbcTournamentDAO">
		<property name="dataSource" ref="dataSource" />
	</bean>
	
	<bean id="matchDAO" class="main.java.com.getsetgo.dao.impl.JdbcMatchDAO">
		<property name="dataSource" ref="dataSource" />
	</bean>
	
	<bean id="gameDAO" class="main.java.com.getsetgo.dao.impl.JdbcGameDAO">
		<property name="dataSource" ref="dataSource" />
	</bean>

</beans>